{
  "expo": {
    "name": "Location Tracker",
    "slug": "location-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kuldeepsahoo.locationtracker",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to track your GPS coordinates.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location to track your GPS coordinates even when the app is in the background.",
        "UIBackgroundModes": [
          "location"
        ]
      }
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION",
        "WAKE_LOCK",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION"
      ],
      "package": "com.kuldeepsahoo.locationtracker"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Location Tracker to use your location to track GPS coordinates even when the app is in the background.",
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      [
        "expo-task-manager"
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "87c231ed-dc09-4019-8706-63c938d2e9d5"
      }
    },
    "owner": "kuldeepsahoohero"
  }
}
